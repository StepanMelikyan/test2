
public enum TypeOfPet {
    
    Cat,
    Dog,
    Hamster;

    public static TypeOfPet getType (int typeId){
        switch (typeId){
            case 1:
                return PetType.Cat;
            case 2:
                return PetType.Dog;
            case 3:
                return PetType.Hamster;
            default:
                return null;
        }
    }
}